<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>


<StackPane fx:controller="controller.HomeScreenController" xmlns:fx="http://javafx.com/fxml" prefHeight="200.0" prefWidth="400.0">
    <VBox alignment="CENTER" spacing="10" >
        <Label text="Welcome to the Home Screen!" />
        <Label fx:id="welcomeLabel" text="abc" />
        <Button text="Logout" onAction="#handleLogoutButtonAction" styleClass="button" />
        <Button text="PrintUserInfo" onAction="#printUserInfo" />
        <Button text="Create New List" onAction="#createNewListAction" />
    </VBox>
</StackPane>
